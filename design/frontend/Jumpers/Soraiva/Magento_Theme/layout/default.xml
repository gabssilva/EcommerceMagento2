<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/Logo_Soraiva.png</argument>
                <argument name="logo_img_width" xsi:type="number">210</argument>
                <argument name="logo_img_height" xsi:type="number">88</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="main" remove="false"/>
        <referenceBlock name="page.main.title" remove="false"/>
        <referenceBlock class="Magento\Newsletter\Block\Subscribe" name="form.subscribe" remove="true"/>

    </body>

</page>